<!DOCTYPE html>

<title>Event</title>

{% block body %}
    <h1>Event</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ event.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ event.name }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ event.createdAt ? event.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FinishAt</th>
                <td>{{ event.finishAt ? event.finishAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
                <th>FinishAt</th>
                <td>{{ event.finishAt ? event.finishAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>StartAt</th>
                <td>{{ event.startAt ? event.startAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Result</th>
                <td>{{ event.result }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('front_app_event_index') }}">back to list</a>

    <a href="{{ path('front_app_event_edit', {'id': event.id}) }}">edit</a>

    {{ include('event/_delete_form.html.twig') }}
{% endblock %}
