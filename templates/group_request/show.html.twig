<!DOCTYPE html>

<title>GroupRequest</title>

{% block body %}
    <h1>GroupRequest</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ group_request.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ group_request.date ? group_request.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ group_request.createdAt ? group_request.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>IsApproved</th>
                <td>{{ group_request.isApproved ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_group_request_index') }}">back to list</a>

    <a href="{{ path('app_group_request_edit', {'id': group_request.id}) }}">edit</a>

    {{ include('group_request/_delete_form.html.twig') }}
{% endblock %}
